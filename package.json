{
  "name": "sabaiydev-portfilio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:server": "cd apps/server && bun --hot src/server/index.ts",
    "db:push": "cd apps/server && bunx drizzle-kit push",
    "db:generate": "cd apps/server && bunx drizzle-kit generate",
    "db:migrate": "cd apps/server && bunx drizzle-kit migrate",
    "db:studio": "cd apps/server && bunx drizzle-kit studio",
    "dev:landing": "cd apps/web-landing && bun run dev",
    "build:landing": "cd apps/web-landing && bun run build",
    "dev:admin": "cd apps/web-admin && bun run dev",
    "build:admin": "cd apps/web-admin && bun run build"
  },
  "devDependencies": {
    "@tanstack/react-query": "5",
    "@trpc/next": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "@hookform/resolvers": "^5.2.2",
    "@trpc/client": "^11.7.1",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^27.0.0",
    "bun": "^1.3.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "js-cookie": "^3.0.5",
    "jsdom": "^27.1.0",
    "moment": "^2.30.1",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.2.3",
    "react-icons": "^5.5.0",
    "superjson": "^2.2.5",
    "zod": "^3.25.76"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}